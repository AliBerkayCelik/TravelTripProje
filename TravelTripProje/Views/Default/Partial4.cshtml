@using TravelTripProje.Models.Siniflar
@model List<Blog>

<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogImage" alt="" style="width:150px;height:80px" />
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0,80) ...</p>
            </div>
            <div class="clear"></div>
        }

        
    </div>
    <div class="box1-left">
       @Html.Action("Partial5","Default")
    </div>
</div>
